<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>RegisterServlet</servlet-name>
        <servlet-class>com.mycompany.arshad.servlet.RegisterServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>RegisterServlet2</servlet-name>
        <servlet-class>com.mycompany.arshad.servlet.RegisterServlet2</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>LoginServlet</servlet-name>
        <servlet-class>com.mycompany.arshad.servlet.LoginServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>OperationServlet1</servlet-name>
        <servlet-class>com.mycompany.arshad.servlet.OperationServlet1</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Operation_Servlet</servlet-name>
        <servlet-class>com.mycompany.arshad.servlet.Operation_Servlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>RegisterServlet</servlet-name>
        <url-pattern>/RegisterServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>RegisterServlet2</servlet-name>
        <url-pattern>/RegisterServlet2</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>LoginServlet</servlet-name>
        <url-pattern>/LoginServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>OperationServlet1</servlet-name>
        <url-pattern>/OperationServlet1</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Operation_Servlet</servlet-name>
        <url-pattern>/Operation_Servlet</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
